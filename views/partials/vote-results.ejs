<div id="current-tally">
  <h5>Vote Results</h5><br>
  <table class="highlight">
    <thead>
      <tr>
        <th>Choice</th>
        <th>Votes</th>
      </tr>
    </thead>
    <tbody>
      <% for (var vote in poll.voteTally) { %>
        <tr>
          <td> <%= vote %></td>
          <td><%= poll.voteTally[vote] %></td>
        </tr>
      <% }; %>
    </tbody>
  </table>
</div>
